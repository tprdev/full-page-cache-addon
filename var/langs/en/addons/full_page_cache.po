msgctxt "Addons::name::full_page_cache"
msgid "Full-page cache"
msgstr "Full-page cache"

msgctxt "Addons::description::full_page_cache"
msgid "Adds an ability to use full-page cache using Varnish."
msgstr "Adds an ability to use full-page cache using Varnish."

msgctxt "Languages::full_page_cache.unable_to_enable_full_page_caching"
msgid "Unable to enable full-page caching."
msgstr "Unable to enable full-page caching."

msgctxt "Languages::full_page_cache.unable_to_connect_to_varhish"
msgid "Unable to connect to Varnish server. Please make sure that Varnish service is running and check the entered address, port and secret."
msgstr "Unable to connect to Varnish server. Please make sure that Varnish service is running and check the entered address, port and secret."

msgctxt "Languages::full_page_cache.error_session_auto_start_enabled"
msgid "The \"session.auto_start\" PHP configuration option is enabled. Please, disable this option by setting \"session.auto_start = 0\" in your php.ini file."
msgstr "The \"session.auto_start\" PHP configuration option is enabled. Please, disable this option by setting \"session.auto_start = 0\" in your php.ini file."

msgctxt "Languages::full_page_cache.error_cant_write_to_varnish_vcl_directory"
msgid "The directory <b>\"[directory]\"</b>, which is used to store Varnish configuration files, does not exists or not writable for current PHP process. Please make sure that directory exists and has correct write permissions set."
msgstr "The directory <b>\"[directory]\"</b>, which is used to store Varnish configuration files, does not exists or not writable for current PHP process. Please make sure that directory exists and has correct write permissions set."

msgctxt "Languages::full_page_cache.notice_caching_was_enabled"
msgid "Full-page caching have been successfully enabled, started and running."
msgstr "Full-page caching have been successfully enabled, started and running."

msgctxt "Languages::full_page_cache.warning_cache_works_only_for_storefront"
msgid "Full-page caching will only work for <b>\"[storefront]\"</b> storefront."
msgstr "Full-page caching will only work for <b>\"[storefront]\"</b> storefront."

msgctxt "Languages::full_page_cache.warning_vcl_regeneration_required"
msgid "Varnish configuration file must be regenerated because you have changed caching-related settings. Please, disable and enable the \"Full-page cache\" add-on in order to your storefront work correctly."
msgstr "Varnish configuration file must be regenerated because you have changed caching-related settings. Please, disable and enable the \"Full-page cache\" add-on in order to your storefront work correctly."

